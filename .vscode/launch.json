{
   // Use IntelliSense to find out which attributes exist for C# debugging
   // Use hover for the description of the existing attributes
   // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
   "version": "0.2.0",
   "configurations": [
        {
            "name": "Launch AsmConsole example",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildAsmConsole",
            "program": "${workspaceFolder}/Examples/AsmConsole/bin/Debug/netcoreapp2.2/AsmConsole.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Examples/AsmConsole",
            "console": "externalTerminal",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Launch NES example",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildNES",
            "program": "${workspaceFolder}/Examples/NES/bin/Debug/netcoreapp2.2/NES.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Examples/NES",
            "console": "externalTerminal",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ,]
}